  "license": "ISC",
  "description": "",
  "botVersion": "0.0.2",
  "mainVersion": "0.0.2",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --cls --respawn --transpile-only --exit-child --watch src -- src/main/index.ts --nq",
    "ts-node": "ts-node --transpile-only src/main/index.ts",
    "start": "yarn && tsc && cross-env NODE_ENV=production node ./dist/main/index.js --nq",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "yarn && tsc && yarn package",
    "package": "concurrently \"yarn package:main\" \"yarn package:bot\"",
    "package:main": "run-script-os",
    "package:bot": "run-script-os",
    "package:main:default": "pkg src/main --compress GZip --output executables/main/$npm_package_name-$npm_package_mainVersion",
    "package:bot:default": "pkg src/bot --compress GZip --output executables/bot/bot-$npm_package_botVersion",
    "package:main:win32": "pkg src/main --compress GZip --output executables/main/%npm_package_name%-%npm_package_mainVersion%",
    "package:bot:win32": "pkg src/bot --compress GZip --output executables/bot/bot-%npm_package_botVersion%",
    "upload": "node ./scripts/upload.js"
  },
  "dependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "appdata-path": "^1.0.0",
    "axios": "^1.6.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "inquirer": "^8.0.0",
    "ip": "^2.0.1",
    "jsonc": "^2.0.0",
    "mineflayer": "^4.20.0",
    "mineflayer-pathfinder": "^2.4.5",
    "ngrok": "^5.0.0-beta.2",
    "node-machine-id": "^1.1.12",
    "prismarine-auth": "^2.4.1",
    "reflect-metadata": "^0.2.1",
    "socket.io": "^4.7.4",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/ip": "^1.1.3",
    "@types/node": "^20.11.1",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "pkg": "^5.8.1",
    "run-script-os": "^1.1.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
